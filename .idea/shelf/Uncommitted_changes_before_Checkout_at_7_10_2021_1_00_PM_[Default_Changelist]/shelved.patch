+                                    </span> <font color="#000" size="+1"><a href="chitietkhoahoc.html">${data[i].description}</a>
+                                    </font> </div>
+                                    <hr>`;
+                // <a href="chitietkhoahoc.html">${data[i].description}</a>
+            }
+            c += `</div>`
+            document.getElementById("courseList").innerHTML = c;
+        }
+    });
+}
